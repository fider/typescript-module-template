{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-var-requires": false,
        "member-ordering": false,
        "typedef-whitespace": false,
        "interface-name": false,
        "quotemark": [true, "single", "backtick"],
        "only-arrow-functions": [false, "allow-declarations", "allow-named-functions"],
        "no-consecutive-blank-lines": [true, 3],
        "ordered-imports": false,
        "variable-name": false,
        "member-access": [true, "no-public"],
        "one-line": [true, "check-catch", "check-finally", "check-open-brace", "check-whitespace"],
        "max-line-length": false,
        "trailing-comma": false,
        "eofline": false,
        "interface-over-type-literal": false,
        "object-literal-sort-keys": false,
        "prefer-const": false
    },
    "rulesDirectory": []
}
